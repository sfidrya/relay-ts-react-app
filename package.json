{
  "name": "sf-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^8.11.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.17",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "babel-plugin-relay": "^12.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "history": "4.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-relay": "^12.0.0",
    "react-router": "^5.2.1",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "relay-runtime": "^12.0.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "convert-sass": "node sassToLess.js",
    "start": "concurrently --kill-others \"yarn run relay --watch\" \"craco start\" \"yarn watch-sass\"",
    "build": "yarn run relay && craco build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write .",
    "postinstall": "husky install",
    "relay": "relay-compiler $@",
    "watch-sass": "nodemon --watch antd.customize.scss --exec \"yarn convert-sass && craco start\" --ext scss",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.3.0",
    "@types/history": "^4.7.9",
    "@types/react-relay": "^11.0.2",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.3.1",
    "@types/relay-runtime": "^12.0.0",
    "@types/testing-library__jest-dom": "^5.14.1",
    "antd": "^4.16.13",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^6.3.0",
    "craco-antd": "^1.19.0",
    "craco-less": "^1.20.0",
    "graphql": "^15.6.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.3",
    "nodemon": "^2.0.13",
    "prettier": "2.4.1",
    "relay-compiler": "^12.0.0",
    "relay-compiler-language-typescript": "^14.2.0",
    "relay-config": "^12.0.0",
    "sass": "^1.42.1"
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx,json}": "prettier --write"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ]
  }
}
